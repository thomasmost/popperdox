{
    "name": "popperdox",
    "version": "0.1.2",
    "description": "create an app to consume rust-generated wasm packages",
    "main": "index.js",
    "engines": {
      "node": ">=8.0.0",
      "pnpm": "9.9.0"
    },
    "packageManager": "pnpm@9.9.0",
    "bin": {
      "popperdox": ".bin/popperdox.js"
    },
    "scripts": {
      "build": "wasm-pack build",
      "dev": "pnpm --dir www run dev",
      "redev": "wasm-pack build && pnpm --dir www install && pnpm --dir www run dev"
    },
    "keywords": [
      "webassembly",
      "wasm",
      "rust",
      "webpack"
    ],
    "author": "Thomas Moore <thomas@versa.org>",
    "dependencies": {},
    "devDependencies": {}
  }
  